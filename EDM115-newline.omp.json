{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "async": true,
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#eae7de",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "background": "#00040e",
          "foreground": "#eae7de",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .UserName }}",
          "type": "session"
        },
        {
          "background": "#00040e",
          "foreground": "#ff5555",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "\udb85\udc0b",
          "type": "root"
        },
        {
          "background": "#00040e",
          "foreground": "#eae7de",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "{{ .Icon }}  ",
          "type": "os"
        },
        {
          "background": "#bd93f9",
          "foreground": "#00040e",
          "options": {
            "display_root": true,
            "style": "full"
          },
          "style": "diamond",
          "template": " \udb80\ude56 {{ path .Path .Location }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#f1fa8c",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "\udb80\udd09 {{ .CurrentDate | date .Format }} \u2022",
          "type": "time"
        },
        {
          "background": "#f1fa8c",
          "foreground": "#00040e",
          "options": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "diamond",
          "template": " \ueba2 {{ .FormattedMs }}",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        },
        {
          "background": "#ffb86c",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "\ue725 ({{ url .UpstreamIcon .UpstreamURL }} {{ url .HEAD .UpstreamURL }}){{ if gt .Ahead 0 }}<#50fa7b> +{{ .Ahead }}</>{{ end }}{{ if gt .Behind 0 }}<#ff5555> -{{ .Behind }}</>{{ end }}{{ if .Working.Changed }}<#eae7de> \uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }}<#eae7de> \uf046 {{ .Staging.String }}</>{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "background": "#50fa7b",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h"
          },
          "style": "diamond",
          "template": "\ued0d {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#ff5555",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h",
            "display_mode": "files",
            "extensions": [
              "package-lock.json"
            ]
          },
          "style": "diamond",
          "template": "\ue71e {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "npm"
        },
        {
          "background": "#ffb86c",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h",
            "display_mode": "files",
            "extensions": [
              "pnpm-lock.yaml"
            ]
          },
          "style": "diamond",
          "template": "\ue865 {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "pnpm"
        },
        {
          "background": "#ff79c6",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h",
            "display_mode": "files",
            "extensions": [
              "bun.lockb",
              "bun.lock"
            ]
          },
          "style": "diamond",
          "template": "\ue76f {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "bun"
        },
        {
          "background": "#50fa7b",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h",
            "display_mode": "files",
            "extensions": [
              "deno.json",
              "deno.jsonc"
            ]
          },
          "style": "diamond",
          "template": "\ue7c0 {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "deno"
        },
        {
          "background": "#50fa7b",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h",
            "display_mode": "files",
            "extensions": [
              "yarn.lock"
            ]
          },
          "style": "diamond",
          "template": "\ue8ec {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "yarn"
        },
        {
          "background": "#50fa7b",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h",
            "display_mode": "files"
          },
          "style": "diamond",
          "template": "\ue73c {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#ff79c6",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h"
          },
          "style": "diamond",
          "template": "\ue738 {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "java"
        },
        {
          "background": "#6272a4",
          "foreground": "#eae7de",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h"
          },
          "style": "diamond",
          "template": "\ue826 {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "lua"
        },
        {
          "background": "#ffb86c",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h"
          },
          "style": "diamond",
          "template": "\ue7a8 {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "rust"
        },
        {
          "background": "#ffb86c",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h"
          },
          "style": "diamond",
          "template": "\ue8ef {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "zig"
        },
        {
          "background": "#8be9fd",
          "foreground": "#00040e",
          "leading_diamond": "\ue0b6",
          "options": {
            "display_mode": "files"
          },
          "style": "diamond",
          "template": "\ue7b0 {{ .Context }}",
          "trailing_diamond": "\ue0b4",
          "type": "docker"
        },
        {
          "background": "#6272a4",
          "foreground": "#eae7de",
          "leading_diamond": "\ue0b6",
          "options": {
            "cache_duration": "2h"
          },
          "style": "diamond",
          "template": "\ue73d {{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "php"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#eae7de",
          "options": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "\u2570\u2500 \u276f\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "enable_cursor_positioning": true,
  "final_space": true,
  "patch_pwsh_bleed": true,
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#eae7de",
    "template": "    \u276f "
  },
  "shell_integration": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#eae7de",
    "newline": true,
    "template": "   \u276f\u276f {{ .AbsolutePWD }}\n    \u276f "
  },
  "upgrade": {
    "auto": false,
    "interval": "168h",
    "notice": false,
    "source": "cdn"
  },
  "version": 4
}
